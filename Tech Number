# Tech Number
def is_tech_number(n):
    num_str = str(n)
    if len(num_str) % 2 != 0:
        return False
    first_half = int(num_str[:len(num_str)//2])
    second_half = int(num_str[len(num_str)//2:])
    return (first_half + second_half) ** 2 == n

number = 2025
print(f"{number} is a tech number:", is_tech_number(number))
